<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>

<ToolBar fx:id="toolBar" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <items>
        <Button mnemonicParsing="false">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../imgs/new.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button mnemonicParsing="false">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../imgs/save.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button mnemonicParsing="false">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../imgs/find.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button mnemonicParsing="false">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../imgs/print.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button mnemonicParsing="false">
            <graphic>
                <ImageView>
                    <image>
                        <Image url="@../imgs/setting.png" />
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </items>
</ToolBar>
