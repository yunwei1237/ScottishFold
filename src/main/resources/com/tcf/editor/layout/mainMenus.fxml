<MenuBar fx:id="menuBar" xmlns:fx="http://javafx.com/fxml/1">
    <menus>
        <Menu fx:id="file" mnemonicParsing="false" text="%menu.file">
            <items>
                <MenuItem fx:id="new" mnemonicParsing="false" text="%menu.file.new" >
